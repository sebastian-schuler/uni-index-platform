{
    "files.exclude": {
        "**/.git/": true,
        ".next/": true,
        "node_modules/": true,
        "out/": true,
        "typings/": true,
        "LICENSE": true,
        "README.md": true,
        ".gitattributes": true,
        ".eslintrc.json": true,
        "next-env.d.ts": true
    },
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.patterns": {
        "*.ts": "${capture}.js",
        "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
        "*.jsx": "${capture}.js",
        "*.tsx": "${capture}.ts",
        ".eslintrc.*": ".eslintignore, .editorconfig, .prettierrc",
        "next.config.js": "i18n.js"
    },
}